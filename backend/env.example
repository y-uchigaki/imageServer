# Database
# 本番環境
# DATABASE_URL=postgres://user:password@localhost:5432/imageserver?sslmode=disable
# ローカル開発（Docker Compose使用時）
DATABASE_URL=postgres://imageserver:imageserver@localhost:5432/imageserver?sslmode=disable

# AWS Configuration
# 本番環境
# AWS_REGION=ap-northeast-1
# AWS_S3_BUCKET=your-bucket-name
# AWS_CLOUDFRONT_URL=https://your-cloudfront-domain.cloudfront.net
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key

# ローカル開発（LocalStack使用時）
AWS_REGION=ap-northeast-1
AWS_S3_BUCKET=imageserver-bucket
AWS_CLOUDFRONT_URL=http://localhost:4566/imageserver-bucket
AWS_ENDPOINT_URL=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# Server
PORT=8080
